{
  "expo": {
    "name": "ChatterNestAI",
    "slug": "ChatterNestAI",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/img.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/img.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/img.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.mussa.chatternestai"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/img.png"
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "eas": {
        "projectId": "80225362-683b-4f82-9486-2eb754344add"
      }
    }
  }
}
